{
  "name": "@jhrhee/airtable",
  "version": "1.0.0",
  "description": "more easier airtable",
  "main": "dist/airtable.service.js",
  "types": "dist/airtable.service.d.ts",
  "license": "MIT",
  "author": "jhrhee <rjchaos@gmail.com>",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "airtable": "^0.11.6",
    "lodash": "^4.17.21",
    "typing": "^0.1.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\..*?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all",
      "<rootDir>/setup-dev.ts"
    ]
  }
}
